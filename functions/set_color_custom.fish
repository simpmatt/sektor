function set_color_custom
    set -U fish_color_normal                normal
    set -U fish_color_command               6ff
    set -U fish_color_param                 fff
    set -U fish_color_redirection           999
    set -U fish_color_comment               555
    set -U fish_color_error                 555
    set -U fish_color_escape                6ff
    set -U fish_color_operator              6ff
    set -U fish_color_end                   fff
    set -U fish_color_quote                 f00
    set -U fish_color_autosuggestion        555
    set -U fish_color_valid_path            --underline
    set -U fish_color_cwd                   6ff
    set -U fish_color_cwd_root              6ff
    set -U fish_color_match                 6ff
    set -U fish_color_search_match          --background=222
    set -U fish_color_selection             --background=222
    set -U fish_pager_color_prefix          f00
    set -U fish_pager_color_completion      fff
    set -U fish_pager_color_description     555 6ff
    set -U fish_pager_color_progress        6ff
    set -U fish_color_history_current       6ff
end
